# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'DoctorPortal' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DoctorPortal

pod ‘Firebase’
pod ‘Firebase/Auth’
pod 'Firebase/Database'

# Pods for Facebook
pod 'Bolts'
pod 'FBSDKCoreKit'
pod 'FBSDKLoginKit'

# Pods for Calendar
pod 'FSCalendar'
pod 'ActionSheetPicker-3.0'

  target 'DoctorPortalTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DoctorPortalUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
